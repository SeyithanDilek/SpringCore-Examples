<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
					 http://www.springframework.org/schema/beans/spring-beans.xsd 
					 http://www.springframework.org/schema/context 
					 http://www.springframework.org/schema/context/spring-context.xsd">
	<context:annotation-config/>
	
	<bean id="Classical" class="autowired.Category">
		<property name="categoryName" value="Classical"/>
		<property name="numberOfCategory" value="100"/>
	</bean>
	
	<bean id="Life" class="autowired.Category">
		<property name="categoryName" value="Life"/>
		<property name="numberOfCategory" value="101"/>
	</bean>
	
	<bean id="Alamut" class="autowired.Book"/>
	
	
	


</beans>